unordered_map<int,int> m;
    vector<int> ans_vector;
    for(int i=0;i<nums.size();i++){
        if(m.find(target-nums[i]) != m.end()){
            ans_vector.push_back(m[target-nums[i]]);
            ans_vector.push_back(i);
            break;
        }
        m[nums[i]] = i;
    }
    return ans_vector;
